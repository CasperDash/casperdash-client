		"reselect": "^4.0.0",
		"sass": "^1.26.10",
		"ts-results": "^3.3.0"
	},
	"reactSnapshot": {
		"exclude": [
			"/dashboard/**"
		]
	},
	"scripts": {
		"dev": "webpack-dev-server --mode development",
		"dev-extension": "webpack --mode development --env=type=extension --watch",
		"dev-extension-mainnet": "webpack --mode development --env=type=extension --env=network=mainnet --watch",
		"dev-extension-testnet": "webpack --mode development --env=type=extension --env=network=testnet --watch",
		"start-mainnet": "webpack-dev-server --mode development --env=network=mainnet",
		"start-testnet": "webpack-dev-server --mode development --env=network=testnet",
		"build-mainnet": "webpack --mode production --env=network=mainnet && react-snapshot",
		"build-testnet": "webpack --mode production  --env=network=testnet && react-snapshot",
		"build-extension-testnet": "webpack --mode production --env=type=extension --env=network=testnet",
		"build-extension-mainnet": "webpack --mode production --env=type=extension --env=network=mainnet",
		"test": "react-scripts test --coverage --watchAll ",
		"test-ci": "CODECOV_TOKEN=f53cf772-9742-494e-af36-d84321b726dc SKIP_PREFLIGHT_CHECK=true CI=true react-scripts test --coverage"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"transform": {
			"^.+\\.[jt]sx?$": "babel-jest"
		},
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!<rootDir>/node_modules/"
		],
		"resetMocks": false,
		"moduleNameMapper": {
			"^.+\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
			"^@cd/assets(.*)$": "<rootDir>/src/assets$1",
			"^@cd/actions(.*)$": "<rootDir>/src/actions$1",
			"^@cd/helpers(.*)$": "<rootDir>/src/helpers$1",
			"^@cd/selectors(.*)$": "<rootDir>/src/selectors$1",
			"^@cd/services(.*)$": "<rootDir>/src/services$1",
			"^@cd/shared(.*)$": "<rootDir>/src/shared$1",
			"^@cd/store(.*)$": "<rootDir>/src/store$1",
			"^@cd/config(.*)$": "<rootDir>/src/config$1",
			"^@cd/constants(.*)$": "<rootDir>/src/constants$1",
			"^@cd/common(.*)$": "<rootDir>/src/components/Common$1",
			"^@cd/hooks(.*)$": "<rootDir>/src/components/hooks$1",
			"^@cd/web/(.*)$": "<rootDir>/src/components/web$1",
			"^@cd/web-extension(.*)$": "<rootDir>/src/components/web-extension$1",
			"^@cd/components(.*)$": "<rootDir>/src/components$1"
		},
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!lodash-es)"
		]
	},
	"devDependencies": {
		"@svgr/webpack": "^6.1.2",
		"@testing-library/react-hooks": "^8.0.1",
		"babel-core": "^6.26.3",
		"babel-jest": "^28.1.3",
		"babel-loader": "^8.2.3",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"copy-webpack-plugin": "^10.2.0",
		"css-loader": "^6.5.1",
		"css-minimizer-webpack-plugin": "^3.2.0",
		"esbuild-loader": "^2.17.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"html-loader": "^3.0.1",
		"html-webpack-plugin": "^5.5.0",
		"jest-extended": "^3.0.1",
		"mini-css-extract-plugin": "^2.4.5",
		"react-select-event": "^5.3.0",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.3.1",
		"webpack": "^5.65.0",
		"webpack-bundle-analyzer": "^4.5.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.6.0",
		"webpack-merge": "^5.8.0"
	}
}
